{
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "Polymer": true
  },
  "rules": {
    // 0 is off
    // 1 is warning
    // 2 is error (stops execution)
    // Possible Errors
    "no-comma-dangle": 1,
    "no-cond-assign": [ 1, "except-parens" ],
    "no-console": 0,
    "no-constant-condition": 1,
    "no-control-regex": 1,
    "no-debugger": 1,
    "no-dupe-keys": 2,
    "no-empty": 1,
    "no-empty-class": 1,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 1,
    "no-extra-parens": 1,
    "no-extra-semi": 1,
    "no-func-assign": 1,
    "no-inner-declarations": [ 1, "functions" ],
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 2,
    "no-regex-spaces": 2,
    "no-reserved-keys": 1,
    "no-sparse-arrays": 2,
    "no-unreachable": 2,
    "use-isnan": 2,
    "valid-jsdoc": 1,
    "valid-typeof": 2
    // Best Practices
    // TODO
  }
}
